{
    "overall": {
        "name": "cats_v_dogs",
        "loss_fn": "sigmoid",
        "experiment_dir": "trial_01",
        "saver": {
            "save_params_name": "best_params_saver"
        },
        "logging": {
            "console": {
                "level": "critical",
                "format_str": null
            },
            "file": {
                "level": "debug",
                "format_str": null
            },
            "graph_spec": true
        },
        "num_classes": 1,
        "metrics": {
            "type": "classification"
        }
    },
    "data": {
        "in": {
            "dim": [
                150,
                150,
                3
            ],
            "dtype": "float32"
        },
        "label": {
            "dim": [
                1
            ],
            "dtype": "int64"
        },
        "TFR_parse": {
            "feature": {
                "name": "/image",
                "dtype": "float32",
                "tftype": "fixedlenfeature",
                "in_type": "string"
            },
            "label": {
                "name": "/label",
                "dtype": "int64",
                "tftype": "fixedlenfeature",
                "in_type": "int64"
            }
        },
        "TFR": {
            "dir": "./examples/cats_v_dogs/data/record_holder/150",
            "train": "train.tfrecords",
            "validation": "val.tfrecords",
            "test": "test.tfrecords"
        },
        "image": {
            "standardize": true,
            "augmentation": {
                "aug_val": true,
                "v_flip": true,
                "h_flip": true,
                "max_brightness": 0.2
            }
        }
    },
    "hyper_parameters": {
        "optimizer": "adam",
        "lr": 0.0001,
        "batch_size": 32,
        "epochs": 20,
        "default_activation": "elu",
        "shuffle_buffer": 128,
        "early_stopping": {
            "epochs": 3,
            "warm_up_epochs": 5
        }
    },
    "hidden": {
        "path": "./examples/cats_v_dogs/hidden_config.json"
    }
}